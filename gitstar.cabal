Name:           gitstar
Version:        0.1.2
build-type:     Simple
License:        GPL-2
License-File:   LICENSE
Author:         HAils team
Maintainer:     Hails team <hails at scs.stanford dot edu>
Stability:      experimental
Synopsis:       Simple GitStar Git-based wiki
Category:       Web
Cabal-Version:  >= 1.6

Description:
        GitStar app

X-Hails-Server: Gitstar

Library
  Build-Depends: base >= 4.5,
                 containers >= 0.4.2,
                 bytestring >= 0.9,
                 binary >= 0.5.1,
                 bson >= 0.2.1,
                 transformers >= 0.2.2,
                 base64-bytestring >= 0.1.1.2,
                 filepath >= 1.3,
                 lio >= 0.9,
                 hails >= 0.9,
                 http-types >= 0.7,
                 -- hails-lite >= 0.1,
                 aeson >= 0.6.0.2,
                 split >= 0.1.4.2,
                 text >= 0.11.2.3,
                 blaze-html >= 0.4.3.3,
                 gitstar-policy == 0.1.2

  Ghc-Options: -Wall -fno-warn-orphans -fno-warn-unused-do-bind
  --Extensions: Safe
  Other-Modules: Gitstar
